{% extends 'base-back.html.twig' %}


{% block title %}Themes{% endblock %}

{% block body %}
    <h1>Themes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ theme.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ theme.nom }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ theme.libelle }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_themes_index') }}">back to list</a>

    <a href="{{ path('app_themes_edit', {'id': theme.id}) }}">edit</a>

    {{ include('themes/_delete_form.html.twig') }}
{% endblock %}
